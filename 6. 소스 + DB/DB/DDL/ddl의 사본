--------------------------------------------------------
--  DDL for View VTBLBOARDM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VTBLBOARDM" ("BOARDM_SEQ", "POSTM_TITLE", "BOARDM_CONTENT", "BOARDM_DATE", "BOARDM_VIEW", "BOARDM_UP", "MEMBER_NAME", "MEMBER_ID", "COMMUNITYC_NAME", "ISNEW", "COMMENTCOUNT") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select 
    b.boardm_seq as boardm_seq ,
    b.postm_title as postm_title,
    b.boardm_content as boardm_content ,
    b.boardm_date as boardm_date ,
    b.boardm_view as boardm_view,
    b.boardm_up as boardm_up ,
    a.member_name as member_name,
    a.member_id as member_id,
    c.communityc_name as communityc_name,
    (sysdate-boardm_date) isnew,
    (select count(*) from tblCommentM where postm_seq = b.boardm_seq) as commentcount
from tblMember a inner join tblBoardM b on a.member_seq = b.memberm_seq
inner join tblCommunityC c on b.communityc_seq = c.communityc_seq
;
--------------------------------------------------------
--  DDL for View VTBLCOMMENTM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VTBLCOMMENTM" ("COMMENTM_SEQ", "COMMENTM_NAME", "COMMENTM_CONTENT", "COMMENTM_DATE", "COMMENTM_UP", "POSTM_SEQ", "MEMBERM_SEQ", "MEMBER_ID") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select tblCommentM."COMMENTM_SEQ",tblCommentM."COMMENTM_NAME",tblCommentM."COMMENTM_CONTENT",tblCommentM."COMMENTM_DATE",tblCommentM."COMMENTM_UP",tblCommentM."POSTM_SEQ",tblCommentM."MEMBERM_SEQ", (select member_id from tblMember where member_seq = tblCommentM.memberm_seq) as member_id from tblCommentM
;
--------------------------------------------------------
--  DDL for View VTBLMINIPM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VTBLMINIPM" ("MINIPM_SEQ", "MINIPM_NAME", "MINIPM_COUNT", "MINIPM_START", "MINIPM_PRICE1", "MINIPM_PRICE2", "MINIPM_PRICE3", "MINICTG_SEQ", "THUMBNAILI_SEQ", "AREA_SEQ", "MINIPM_END", "MSTATUS", "MINICTG_NAME", "THUMBNAILI_NAME", "AREA_NAME", "REC") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select a."MINIPM_SEQ",a."MINIPM_NAME",a."MINIPM_COUNT",a."MINIPM_START",a."MINIPM_PRICE1",a."MINIPM_PRICE2",a."MINIPM_PRICE3",a."MINICTG_SEQ",a."THUMBNAILI_SEQ",a."AREA_SEQ",a."MINIPM_END",a."MSTATUS",(select b.minictg_name from tblMiniCtg b where b.minictg_seq = a.minictg_seq) as minictg_name
          ,(select c.thumbnaili_name from tblThumbnailI c where c.thumbnaili_seq = a.thumbnaili_seq) as thumbnaili_name
          ,(select d.area_name from tblArea d where d.area_seq = a.area_seq) as area_name 
          ,(select e.rec from tblMiniRec e where e.rec='y' and e.minipm_seq = a.minipm_seq) as rec 
          from tblMiniPM a
;
--------------------------------------------------------
--  DDL for View VTBLPACKAGEPM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VTBLPACKAGEPM" ("PKGPM_SEQ", "PKGPM_NAME", "PKGPM_COUNT", "PKGPM_START", "PKGPM_END", "PKGPM_ADULTPRICE", "PKGPM_KIDPRICE", "PKGPM_TODDLERPRICE", "THUMBNAILI_SEQ", "AREA_SEQ", "PKGPM_PERIOD", "PSTATUS", "REC", "THUMBNAILI_NAME", "AREA_NAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select a."PKGPM_SEQ",a."PKGPM_NAME",a."PKGPM_COUNT",a."PKGPM_START",a."PKGPM_END",a."PKGPM_ADULTPRICE",a."PKGPM_KIDPRICE",a."PKGPM_TODDLERPRICE",a."THUMBNAILI_SEQ",a."AREA_SEQ",a."PKGPM_PERIOD",a."PSTATUS",
(select b.rec from tblPackageRec b where b.rec = 'y' and b.pkgpm_seq = a.pkgpm_seq) as rec,(select c.thumbnaili_name from tblThumbnailI c where c.thumbnaili_seq = a.thumbnaili_seq) as thumbnaili_name,(select d.area_name from tblArea d where d.area_seq = a.area_seq) as area_name
from tblPackagePM a
;
--------------------------------------------------------
--  DDL for View VWAENDNAME
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWAENDNAME" ("AIRPORT_NAME", "ENDL_SEQ") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select 
    a.airport_name as airport_name,
    e.endl_seq
from tblairport a inner join tblendl  e on e.airport_seq = a.airport_seq
;
--------------------------------------------------------
--  DDL for View VWAIRPLANP
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWAIRPLANP" ("AIRPLANP_SEQ", "AIRPLANL_NAME", "AIRPLANL_SEQ", "AIRPLANP_TAKEOFFDATE", "AIRPLANP_TAKEOFFTIME", "AIRPLANP_NUMBER", "AIRPLANP_ADULTPRICE", "AIRPLANP_KIDPRICE", "AIRPLANP_TODDLERPRICE", "SALEK_SEQ", "SALEK_KIND", "STARTL_SEQ", "SAIRPORT_NAME", "ENDL_SEQ", "EAIRPORT_NAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select  
p.airplanp_seq as airplanp_seq,
l.airplanl_name as airplanl_name,
p.airplanl_seq as airplanl_seq,
p.airplanp_takeoffdate as airplanp_takeoffdate,
p.airplanp_takeofftime as airplanp_takeofftime,
p.airplanp_number as airplanp_number,
p.airplanp_adultprice as airplanp_adultprice,
p.airplanp_kidprice as airplanp_kidprice,
p.airplanp_toddlerprice as airplanp_toddlerprice,
p.salek_seq as salek_seq,
k.salek_kind as salek_kind,
p.startl_seq as startl_seq,
s.airport_name as sairport_name,
p.endl_seq as endl_seq,
e.airport_name as eairport_name
from tblairplanp p ,
       vwastartname s,
       vwaendname e,
       tblsalek k,
       tblairplanl l
       where 
       p.airplanl_seq = l.airplanl_seq and
       p.startl_seq = s.startl_seq and 
       p.endl_seq = e.endl_seq and 
       p.salek_seq = k.salek_seq
;
--------------------------------------------------------
--  DDL for View VWASTARTNAME
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWASTARTNAME" ("AIRPORT_NAME", "STARTL_SEQ") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select 
    a.airport_name  as airport_name,
    s.startl_seq
    from tblairport a inner join tblstartl  s on s.airport_seq = a.airport_seq
;
--------------------------------------------------------
--  DDL for View VWCARLIST
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWCARLIST" ("CARPROD_SEQ", "CARPROD_NAME", "CARPROD_PEOPLE", "CARPROD_NUMBER", "CARPROD_PRICE", "CARMANUFACTURER_SEQ", "CARFUEL_SEQ", "CARKIND_SEQ", "AREA_SEQ", "THUMBNAILI_SEQ", "STATUS", "MANUFNAME", "FUELNAME", "KINDNAME", "AREANAME", "THUMBNAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select tcp."CARPROD_SEQ",tcp."CARPROD_NAME",tcp."CARPROD_PEOPLE",tcp."CARPROD_NUMBER",tcp."CARPROD_PRICE",tcp."CARMANUFACTURER_SEQ",tcp."CARFUEL_SEQ",tcp."CARKIND_SEQ",tcp."AREA_SEQ",tcp."THUMBNAILI_SEQ",tcp."STATUS",tcm.carmanufacturer_name as manufname,tcf.carfuel_name as fuelname, tck.carkind_name as kindname, ta.area_name as areaname,ttb.thumbnaili_name as thumbname from tblcarprod tcp inner join tblcarmanufacturer tcm on tcp.carmanufacturer_seq = tcm.carmanufacturer_seq
inner join tblcarfuel tcf on tcp.carfuel_seq=tcf.carfuel_seq inner join tblcarkind tck on tck.carkind_seq=tcp.carkind_seq inner join tblarea ta on tcp.area_seq=ta.area_seq inner join tblthumbnaili ttb on ttb.thumbnaili_Seq = tcp.thumbnaili_seq
;
--------------------------------------------------------
--  DDL for View VWCOMULIST
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWCOMULIST" ("SEQ", "TITLE", "DAY", "COUNT", "CNAME", "NAME", "MEMSEQ") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select tb.boardm_seq as seq,tb.postm_title as title,
tb.boardm_date as day,tb.boardm_view as count, tcc.communityc_name as cname,tm.member_name name,tb.memberm_seq as memseq from tblboardm tb inner join tblcommunityC tcc on tb.communityc_seq = tcc.communityc_seq
inner join tblmember tm on tm.member_seq=tb.memberm_seq
;
--------------------------------------------------------
--  DDL for View VWCONFIRM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWCONFIRM" ("SEQ", "ADULT", "KID", "TOD", "DAY", "TIME", "NAME", "STATUS", "PRICE", "CONFIRM") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select tr.rez_seq as seq,tr.rez_adultcnt as adult,tr.rez_kidcnt as kid,tr.rez_toddlercnt as tod,tap.airplanp_takeoffdate as day,tap.airplanp_takeofftime as time,tm.member_name as name,tdp.deposit_yn as status,(tap.airplanp_adultprice+tap.airplanp_kidprice+tap.airplanp_toddlerprice) as price, tr.confirm as confirm
from tblairplanP tap inner join tblairplanrez tar on tap.airplanp_seq = tar.airplanpr_seq
inner join tblrez tr on tr.rez_seq = tar.rez_seq 
inner join tblmember tm on tm.member_seq = tr.member_seq
inner join tbldeposit tdp on tdp.deposit_seq = tr.deposit_Seq
;
--------------------------------------------------------
--  DDL for View VWDELBOARD
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWDELBOARD" ("BSEQ", "TITLE", "TCSEQ", "TCCSEQ") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select tb.boardm_seq as bseq,tb.postm_title as title,tcm.commentm_seq as tcseq,tccm.ccommentm_seq as tccseq
from tblboardm tb left outer join tblCommentM tcm on tb.boardm_seq = tcm.postm_seq
left outer join tblCCommentM tccm on tccm.commentm_seq = tcm.commentm_seq
;
--------------------------------------------------------
--  DDL for View VWFLIGHTLIST
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWFLIGHTLIST" ("AIRPLANP_SEQ", "AIRPLANP_TAKEOFFDATE", "AIRPLANP_TAKEOFFTIME", "AIRPLANP_NUMBER", "AIRPLANP_ADULTPRICE", "AIRPLANP_KIDPRICE", "AIRPLANP_TODDLERPRICE", "SALEK_SEQ", "AIRPLANL_SEQ", "STARTL_SEQ", "ENDL_SEQ", "SALEK_KIND", "AIRPLANL_NAME", "STARTPORT", "ENDPORT") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select ta."AIRPLANP_SEQ",ta."AIRPLANP_TAKEOFFDATE",ta."AIRPLANP_TAKEOFFTIME",ta."AIRPLANP_NUMBER",ta."AIRPLANP_ADULTPRICE",ta."AIRPLANP_KIDPRICE",ta."AIRPLANP_TODDLERPRICE",ta."SALEK_SEQ",ta."AIRPLANL_SEQ",ta."STARTL_SEQ",ta."ENDL_SEQ",ts.salek_kind as salek_kind,tl.airplanl_name as airplanl_name,tap2.airport_name as startport, tap.airport_name as endport from tblAirplanp ta inner join tblsalek ts on ta.salek_seq = ts.salek_seq 
inner join tblairplanl tl on ta.airplanl_seq=tl.airplanl_seq 
inner join tblendl te on ta.endl_seq= te.endl_seq
inner join tblAirport tap on te.airport_seq = tap.airport_seq 
inner join tblstartl tsl on ta.startl_seq=tsl.startl_seq
inner join tblAirport tap2 on tsl.airport_seq = tap2.airport_seq
;
--------------------------------------------------------
--  DDL for View VWMINITAGITEM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWMINITAGITEM" ("HASHTAG_SEQ", "MINIPM_SEQ", "THUMBNAILI_SEQ", "AREA_SEQ", "MINIPM_NAME", "MINIPM_COUNT", "MINIPM_START", "MINIPM_PRICE1", "MINIPM_PRICE2", "MINIPM_PRICE3", "MINICTG_SEQ", "MINIPM_END", "MSTATUS", "AREA_NAME", "IMAGEM_SEQ", "THUMBNAILI_NAME", "HASHTAG_NAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select "HASHTAG_SEQ","MINIPM_SEQ","THUMBNAILI_SEQ","AREA_SEQ","MINIPM_NAME","MINIPM_COUNT","MINIPM_START","MINIPM_PRICE1","MINIPM_PRICE2","MINIPM_PRICE3","MINICTG_SEQ","MINIPM_END","MSTATUS","AREA_NAME","IMAGEM_SEQ","THUMBNAILI_NAME","HASHTAG_NAME" from tblminipm
    left join tblArea using(area_seq)
        left join tblminiimg using(MINIPM_SEQ)
            left join tblthumbnaili using(THUMBNAILI_SEQ)
                inner join (select distinct * from tblminitag) using(MINIPM_SEQ)
                    inner join tblhashtag using(hashtag_seq)
        order by MINIPM_SEQ
;
--------------------------------------------------------
--  DDL for View VWPKITEM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWPKITEM" ("THUMBNAILI_SEQ", "PKGPM_SEQ", "AREA_SEQ", "PKGPM_NAME", "PKGPM_COUNT", "PKGPM_START", "PKGPM_END", "PKGPM_ADULTPRICE", "PKGPM_KIDPRICE", "PKGPM_TODDLERPRICE", "PKGPM_PERIOD", "PSTATUS", "AREA_NAME", "IMGM_SEQ", "IMAGEM_SEQ", "IMAGEM_NAME", "THUMBNAILI_NAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select "THUMBNAILI_SEQ","PKGPM_SEQ","AREA_SEQ","PKGPM_NAME","PKGPM_COUNT","PKGPM_START","PKGPM_END","PKGPM_ADULTPRICE","PKGPM_KIDPRICE","PKGPM_TODDLERPRICE","PKGPM_PERIOD","PSTATUS","AREA_NAME","IMGM_SEQ","IMAGEM_SEQ","IMAGEM_NAME","THUMBNAILI_NAME" 
from tblpackagepm 
    left join tblArea using(area_seq)  --지역번호
        left join tblpackagei using(pkgpm_seq) --패키지-이미지
            left join tblimagem on tblimagem.imagem_seq=tblpackagei.imgm_seq --이미지
                left join tblthumbnaili using(thumbnaili_seq) --썸네일
        order by pkgpm_seq
;
--------------------------------------------------------
--  DDL for View VWPKTAGITEM
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "ADMIN"."VWPKTAGITEM" ("HASHTAG_SEQ", "PKGPM_SEQ", "THUMBNAILI_SEQ", "AREA_SEQ", "PKGPM_NAME", "PKGPM_COUNT", "PKGPM_START", "PKGPM_END", "PKGPM_ADULTPRICE", "PKGPM_KIDPRICE", "PKGPM_TODDLERPRICE", "PKGPM_PERIOD", "PSTATUS", "AREA_NAME", "IMGM_SEQ", "IMAGEM_SEQ", "IMAGEM_NAME", "THUMBNAILI_NAME", "HASHTAG_NAME") DEFAULT COLLATION "USING_NLS_COMP"  AS 
  select "HASHTAG_SEQ","PKGPM_SEQ","THUMBNAILI_SEQ","AREA_SEQ","PKGPM_NAME","PKGPM_COUNT","PKGPM_START","PKGPM_END","PKGPM_ADULTPRICE","PKGPM_KIDPRICE","PKGPM_TODDLERPRICE","PKGPM_PERIOD","PSTATUS","AREA_NAME","IMGM_SEQ","IMAGEM_SEQ","IMAGEM_NAME","THUMBNAILI_NAME","HASHTAG_NAME" 
from tblpackagepm 
    left join tblArea using(area_seq)  --지역번호
        left join tblpackagei using(pkgpm_seq) --패키지-이미지
            left join tblimagem on tblimagem.imagem_seq=tblpackagei.imgm_seq --이미지
                left join tblthumbnaili using(thumbnaili_seq) --썸네일
                    inner join (select distinct * from tblpackagetag) using(pkgpm_seq) -- 패키지-해시태그
                        inner join tblhashtag using(hashtag_seq) --해시태그
        order by pkgpm_seq
;
